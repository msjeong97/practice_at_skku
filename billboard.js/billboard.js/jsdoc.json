{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "dist/billboard.js",
      "./README.md"
    ],
    "excludePattern": "(node_modules/|doc/|theme/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "doc/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    }
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}